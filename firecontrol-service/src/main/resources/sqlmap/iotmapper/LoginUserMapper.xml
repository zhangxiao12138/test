<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.firecontrol.mapper.iotmapper.LoginUserMapper" >
    <resultMap id="BaseResultMap" type="com.firecontrol.domain.entity.LoginUser" >
<!-- column 对应数据库字段， property对应实体bean的属性 -->
        <id column="id" property="id" jdbcType="BIGINT" />
        <id column="vendor_id" property="vendorId" jdbcType="BIGINT" />
        <result column="code" property="code" jdbcType="VARCHAR" />
        <result column="username" property="skuCode" jdbcType="VARCHAR" />
        <result column="password" property="skuName" jdbcType="VARCHAR" />
        <result column="create_date" property="createDate" jdbcType="TIMESTAMP" />
        <result column="modify_date" property="modifyDate" jdbcType="TIMESTAMP" />
        <result column="last_login_date" property="lastLoginDate" jdbcType="TIMESTAMP" />
        <result column="df" property="df" jdbcType="INTEGER" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, vendor_id, code,username, password, create_date, modify_date, last_login_date, df
    </sql>

    <insert id="insert" parameterType="com.firecontrol.domain.entity.LoginUser" >
        insert into tb_server_user (vendor_id, code, username, password, create_date, modify_date, last_login_date, df
        )
        values (#{vendorId,jdbcType=BIGINT},#{code,jdbcType=BIGINT}, #{username,jdbcType=VARCHAR},
        #{password,jdbcType=VARCHAR}, #{createDate,jdbcType=TIMESTAMP}, #{modifyDate,jdbcType=TIMESTAMP},
        #{lastLoginDate,jdbcType=TIMESTAMP}, #{df,jdbcType=INTEGER}
        )
    </insert>


</mapper>